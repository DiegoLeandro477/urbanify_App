{
  "expo": {
    "name": "Urbanify",
    "slug": "urbanify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon432x.png",
    "scheme": "urbanify",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/icon432x.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#FFFFFF",
      "hidden": false
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.diegoleandro477.urbanify"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/icon432x.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.diegoleandro477.urbanify"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/icon432x.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-image-picker",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "34543a05-ebde-4e3b-96fe-f7da8e7306cd"
      }
    }
  }
}
